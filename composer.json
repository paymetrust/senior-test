{
  "require-dev": {
    "pestphp/pest": "^1.23",
    "spatie/phpunit-watcher": "^1.23",
    "symfony/var-dumper": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "Domain\\Users\\": "domain/Users/src/",
      "Domain\\Events\\": "domain/Events/src/",
      "Domain\\Booking\\": "domain/Booking/src/",
      "Infrastructures\\Pdo\\": "infrastructures/Pdo/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Domain\\Users\\Tests\\": "domain/Users/tests/",
      "Domain\\Events\\Tests\\": "domain/Events/tests/",
      "Domain\\Booking\\Tests\\": "domain/Booking/tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "beberlei/assert": "^3.3",
    "zircote/swagger-php": "^4.7",
    "doctrine/annotations": "^2.0"
  }
}
